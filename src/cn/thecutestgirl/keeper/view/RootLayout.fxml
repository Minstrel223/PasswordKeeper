<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="460.0" prefWidth="800.0" stylesheets="@DarkTheme.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cn.thecutestgirl.keeper.view.RootLayoutController">
   <top>
      <MenuBar BorderPane.alignment="TOP_CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
                  <MenuItem mnemonicParsing="false" onAction="#handleNew" text="新建" />
                  <MenuItem mnemonicParsing="false" onAction="#handleOpen" text="打开" />
                  <MenuItem mnemonicParsing="false" onAction="#handleSave" text="保存" />
                  <MenuItem mnemonicParsing="false" onAction="#handleSaveAs" text="另存为" />
              <MenuItem mnemonicParsing="false" onAction="#handleExit" text="退出" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#handleAbout" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
